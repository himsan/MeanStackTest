{{!-- create user list --}}

<nav class="navbar navbar-inverse">
    <div class="container-fluid">
        <div class="navbar-header">
            <ul class="nav navbar-nav">
                <li class="active">
                    <a href="/api/bookStore/v0/admin/userlist">Users</a>
                </li>
                <li class="active">
                    <a href="/api/bookStore/v0/admin/booklist">Books</a>
                </li>
                <li>
                    <a href="/api/bookStore/v0/logout" class="logout fa fa-sign-out" aria-hidden="true"></a>
                </li>

            </ul>
        </div>
</nav>

<div class="container">
    <div class="row">
        <h3 class="col-lg-12 page-header">Users list</h3>
    </div>
    <div class="col-lg-12 table-responsive card-block">
        <table style="width:100%">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>List Name</th>
                    <th>Email</th>
                    <th>Phone Number</th>

                </tr>
            </thead>
            <tbody>
                {{#each top_results}}
                <tr>
                    <td>
                        {{this.firstName}}
                    </td>
                    <td>
                        {{this.lastName}}
                    </td>
                    <td>{{this.email}}</td>
                    <td>{{this.phoneNumber}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>